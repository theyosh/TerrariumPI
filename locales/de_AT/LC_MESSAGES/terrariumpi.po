msgid "Timer"
msgstr "Zeitschaltuhr"

#: terrariumTranslations.py:82 terrariumTranslations.py:89
#: terrariumTranslations.py:107 terrariumTranslations.py:120
#: views/dashboard.tpl:83 views/dashboard.tpl:112 views/dashboard.tpl:159
#: views/dashboard.tpl:206 views/inc/usage_environment.tpl:34
